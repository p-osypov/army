{
  "name": "army",
  "version": "0.2.0",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write **/*.{js,ts,tsx,css,json}",
    "prettier:check": "prettier --check **/*.{js,ts,tsx,css,json}",
    "ts:check": "tsc",
    "check": "npx next lint --max-warnings=0 && npm run ts:check && npm run prettier:check",
    "husky:execute": "chmod ug+x .husky/*",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": "npm run prettier"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "formik": "^2.4.6",
    "next": "^14.2.3",
    "next-translate": "^2.6.2",
    "next-translate-plugin": "^2.6.2",
    "react": "^18.3.1",
    "react-click-outside-hook": "^1.1.1",
    "react-dom": "^18.3.1",
    "react-player": "^2.16.0",
    "react-scrollbars-custom": "^4.1.1",
    "react-slick": "^0.30.2",
    "react-toastify": "^10.0.5",
    "slick-carousel": "^1.8.1",
    "styled-components": "^6.1.8",
    "use-debounce": "^10.0.0",
    "uuidv4": "^6.2.13",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.69",
    "@types/react-click-outside-hook": "^1.0.3",
    "@types/react-dom": "^18.2.22",
    "@types/react-slick": "^0.23.13",
    "@types/styled-components": "^5.1.34",
    "@types/youtube": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.10",
    "prettier": "^3.2.5"
  }
}
